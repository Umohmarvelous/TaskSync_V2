{
    "version": 2,
    "builds": [
        {
            "src": "backend/package.json",
            "use": "@vercel/node"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "devCommand": "npm run start:dev",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/backend/dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        }
    ]
}